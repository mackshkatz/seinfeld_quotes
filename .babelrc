{
  "presets": [
    ["env", {"loose": true}],
    "stage-0",
    ["react"]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-decorators",
    "transform-decorators-legacy",
    "transform-function-bind",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
